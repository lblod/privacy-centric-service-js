{{uuid "id"}}
{
	"data": {
		"type": "{{type}}",
		"id": "{{id}}",
		"attributes": {
			{{#if registrationNumber}}
			"registration": "{{registrationNumber}}" {{#if dateOfBirth}},{{/if}}
			{{/if}}
			{{#if dateOfBirth}}
			"date-of-birth": "{{dateOfBirth}}"
			{{/if}}
		},
		"relationships": {
			{{#if genderId}}
			"gender": {
				"data": {
					"type": "genders",
					"id": "{{genderId}}"
				}
			},
			{{/if}}
			"nationalities": {
				"data": [
				
				{{#each nationalities as | nationality |}}
					{
						"type": "nationalities",
						"id": "{{nationality}}"
					} {{#unless @last}},{{/unless}}
				{{/each}}
				]
			}
			{{#if reasonId}}
			,
			"reason": {
				"data": {
					"id": "{{reasonId}}",
					"type": "request-reasons"
				}
			}
			{{/if}}
		}
	}
}
